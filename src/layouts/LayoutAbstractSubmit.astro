---
import "@fontsource-variable/montserrat";
import "../styles/global.css";
import Head from "../components/Head.astro";
import Header from "../components/Header.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Head title={title} />
  <body>
    <Header />
    <slot />
  </body>
</html>
